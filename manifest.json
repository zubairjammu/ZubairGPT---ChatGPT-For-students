{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "ZubiGPT prompts",
  "version": "0.3.0",
  "description": "ZubiGPT prompts is an extention which provide a lot of usefull prompts for using ChatGPT Effectively",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "ZubiGPT Prompts"
  },
  "content_scripts": [
    {
      "matches": [
        "https://chat.openai.com/*"
      ],
      "run_at": "document_idle",
      "js": [
        "content.js"
      ]
    }
  ]
}
